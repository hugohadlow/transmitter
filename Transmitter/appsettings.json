{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Archive": {
    "Location": "archive"
  },
  "Encryption": {
    "HashFunction": "SHA512"
  },
  "Keys": {
    "SigningKey": {
      "Location": "signing-keys",
      "KeyLength": 4096
    },
    "EncryptionKey": {
      "Location": "encryption-keys",
      "KeyLength": 4096
    }
  }
}
