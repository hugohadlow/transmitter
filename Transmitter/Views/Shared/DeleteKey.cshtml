@model Transmitter.Models.Key
@{
    Layout = "_Layout";
}
<!DOCTYPE html>
<html>
<head>
    <title>Key manager</title>
</head>
<body>
    <h1>Delete key</h1>

    <h2>Are you sure you want to delete key "@Model.Name"</h2>
    <form method="post">
        PublicKey: @Model.PublicKey
        <input type="hidden" name="publicKey" value="@Model.PublicKey" />
        <button type="submit">Delete</button>
    </form>
</body>
</html>